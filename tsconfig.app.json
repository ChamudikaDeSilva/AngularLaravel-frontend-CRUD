/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compilerOptions": {
    "baseUrl": "./src",
    "moduleResolution": "node",
    "paths": {
      "@app/*": ["app/*"]
    },
    "outDir": "./out-tsc/app",
    "types": ["node", "jasmine"],
    "target": "es2022",
    "useDefineForClassFields": false
  },
  "include": [
    "src/**/*.ts"
  ],
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true
  }
}



